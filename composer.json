{
    "name": "orkhanahmadov/yandex-checkout",
    "description": "Easy and complete YooKassa (formerly Yandex Checkout) integration for Laravel",
    "keywords": [
        "orkhanahmadov",
        "yandex",
        "yandex-checkout",
        "yookassa",
        "laravel-payment"
    ],
    "homepage": "https://github.com/orkhanahmadov/yandex-checkout",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Orkhan Ahmadov",
            "email": "orkhan@fastmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/container": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/queue": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "yandex-money/yandex-checkout-sdk-php": "^1.6"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Orkhanahmadov\\YandexCheckout\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orkhanahmadov\\YandexCheckout\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orkhanahmadov\\YandexCheckout\\YandexCheckoutServiceProvider"
            ],
            "aliases": {
                "YandexCheckout": "Orkhanahmadov\\YandexCheckout\\Facades\\YandexCheckout"
            }
        }
    }
}
